
<view class="yfh-status">
    <view class="p">奖品已发货</view>
    <view class="p1">{{logisticsName}}  {{logisticsCode}}</view>
</view>

<view class="dfh-address">
    <view class="address-top">
        <view class="top-left">收货人</view>
        <view class="top-msg">{{userName}}  {{userPhone}}</view>
    </view>
     <view class="address-top">
        <view class="top-left">地址</view>
        <view class="top-msg">{{userAddresss}}</view>
    </view>
</view>


<view class="prize-item">
    <view class="item-header">
        <view class="header-left">
            <image class="left-logo" src="{{advertiserIcon}}"></image>
            <view class="left-p">由 {{advertiserName}} 提供</view>
        </view>
    </view>
    <view class="item-content">
        <image class="content-img" src="{{entityIcon}}"></image>
        <view class="content-font">
            <view class="font-p">{{entityName}}</view>
            <view class="font-p1">x1</view>
        </view>
    </view>
    <view class="item-footer">
        <view class="footer-left">领取方式</view>
        <view class="footer-right">快递物流</view>
    </view>
</view>