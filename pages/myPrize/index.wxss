page{
    height: 100%;
    background-color: #f5f5f5;
    
}
.prize-item{
	margin-bottom:19rpx;
	border-bottom:0.5px solid #ececec;
	border-top:0.5px solid #ececec;
	padding:0 35rpx;
    background-color: #fff;
    
}
.prize-item .item-header{
	height:89rpx;
	border-bottom:0.5px solid #ececec;
	display:flex;
	align-items:center;
}
.prize-item .item-header .header-left{
	font-size:26rpx;
	color:#999999;
	max-width:470rpx;
	white-space:nowrap;
    overflow:hidden;
    text-overflow:ellipsis;
    	display:flex;
	align-items:center;
}
.prize-item .item-header .header-left .left-logo{
	width:40rpx;
	height:40rpx;
	border-radius:50%;
	display:inline-block;
}
.prize-item .item-header .header-left .left-p{
	line-height:32rpx;
	padding-left:20rpx;
}
.prize-item .item-header .header-right{
	font-size:26rpx;
	flex:1;
	text-align:right;
}

.prize-item .item-header .red{
	color:#ff5741;
}

.prize-item .item-header .gray{
	color:#999999;
}

.prize-item .item-content{
	display:flex;
	padding:30rpx 0;
}
.prize-item .item-content .content-img{
	width:120rpx;
	height:120rpx;
	border-radius:5rpx;
	margin-right:40rpx;
}
.prize-item .item-content .content-font{
	flex:1;
}
.prize-item .item-content .content-font .font-p{
	font-size:35rpx;
	color:#353535;
	line-height:60rpx;
}
.prize-item .item-content .content-font .font-p1{
	font-size:26rpx;
	color:#999999;
	line-height:50rpx;
}
.prize-item .item-content .content-num{
	font-size:30rpx;
	color:#999999;
}

.no-prize{
	position:fixed;
	top:30%;
	left:50%;
	transform:translate3d(-50%,-50%,0);
	font-size:36rpx;
	color:#323232;
	line-height:48rpx;
}