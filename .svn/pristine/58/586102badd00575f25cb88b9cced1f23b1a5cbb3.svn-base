page {
    height: 100%;
    overflow: hidden;
}

.image-blur {
    width: 100%;
    height: 100%;
    -webkit-filter: blur(57.8rpx);
    filter: blur(57.8rpx);
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate3d(-50%, -50%, 0);
    z-index: -1;
    background-size: 230% 230%;
    background-repeat: no-repeat;
    background-position: center center;
    opacity: 0.7;
}

.room-box {
    display: flex;
    flex-direction: column;
    width: 100%;
    height: 100%;
    overflow: hidden;
    position: relative;
    box-sizing: border-box;
}

.room-box::after {
    content: '';
    z-index: -1;
    position: absolute;
    top: 242rpx;
    left: 0;
    width: 750rpx;
    height: 179rpx;
    background-image: url(https://1251097942.cdn.myqcloud.com/1251097942/platform/miniApp/shake_broadcast_images/bgstyle-icon.png);
    background-size: 100% 100%;
    background-position: center center;
    background-repeat: no-repeat;
}

.room-bg {
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    position: absolute;
    z-index: -2;
    background-color: rgba(43, 43, 43, 1);
}

.center-users .block {
    transition: all ease-out 0.3s;
}




/* top-topic  */

.top-topic {
    text-align: center;
    height: 596rpx;
    position: relative;
    padding-top: 32rpx;
    box-sizing: border-box;
    transition: all ease-out 0.5s;
}

.top-topic .topic {
    font-size: 32rpx;
    color: #fff;
    line-height: 64rpx;
    display: inline-block;
    transition: all ease-out 0.5s;
}

.top-topic .hot-icon {
    font-size: 26rpx;
    color: rgba(255, 255, 255, 0.6);
    line-height: 58rpx;
    display: flex;
    justify-content: center;
    align-items: center;
    width: auto;
    transition: all ease-out 0.5s;
}

.top-topic .hot-icon::before {
    content: '';
    display: inline-block;
    width: 20rpx;
    height: 27rpx;
    background-image: url(https://1251097942.cdn.myqcloud.com/1251097942/platform/miniApp/shake_broadcast_images/hot-icon.png);
    background-size: 20rpx 27rpx;
    background-position: center center;
    background-repeat: no-repeat;
    margin-right: 10rpx;
}

.top-topic .topic-CD {
    position: absolute;
    top: 165rpx;
    left: 50%;
    border-radius: 50%;
    margin-left: -205rpx;
    overflow: hidden;
    /* border: 10rpx solid rgba(255, 255, 255, 0.6); */
    box-shadow: 0 0 0 10rpx rgba(255, 255, 255, 0.6);
    transition: all ease-out 0.5s;
}

.top-topic .topic-CD .CD {
    border-radius: 50%;
    height: 410rpx;
    width: 410rpx;
    background-repeat: no-repeat;
    background-size: 100% 100%;
    background-position: center center;
    overflow: hidden;
}

.top-topic .topic-CD .CD-btn {
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate3d(-50%, -50%, 0);
    width: 88rpx;
    height: 88rpx;
    background-size: 100% 100%;
    background-position: center center;
    background-repeat: no-repeat;
}

.top-topic .topic-CD .play-btn {
