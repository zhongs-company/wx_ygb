
.image-blur {
    width: 100%;
    height: 100%;
    -webkit-filter: blur(100rpx);
    filter: blur(100rpx);
    position: absolute;
    top: 50%;
    left: 50%;
    transform: translate3d(-50%, -50%, 0);
    z-index: -1;
    background-size: 230% 230%;
    background-repeat: no-repeat;
    background-position: center center;
    opacity: 0.7;
}
.room-bg {
    width: 100%;
    height: 100%;
    top: 0;
    left: 0;
    position: absolute;
    z-index: -2;
    background-color: rgba(43, 43, 43, 1);
}
